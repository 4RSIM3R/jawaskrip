fungsi hitung(a, b, ...c){
  var hasil = a ditambah b

  jika (c.length) {
    hasil += c[0]
  }

  setiap (c sebagai tambahan) {
    hasil -= -(tambahan)
  }

  kembalikan hasil
}

kelas Hewan {
  konstruktor () {
    ini.berbicara = salah
  }
}

kelas Sapi turunan Hewan {
  konstruktor () {
    super()
    ini.posisi = 0
  }

  jalan () {
    ini.posisi += 100
    tulis('sapi berjalan,', ini.posisi)
  }
}

