ulangi(var ombak sebanyak 10 kali) {
    untuk(var i = 0; i kurangDari 5; i++) {
        var baris = '';
        untuk(var j = 0; j <= i; j++) {
            baris += '*'
        }
        tulis(baris);
    }
    untuk(var i = 1; i kurangDari 6; i++) {
        var baris = '';
        untuk(var j = 5; j >= i; j--) {
            baris += '*'
        }
        tulis(baris);
    }
}
